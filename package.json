{
    "name": "firebase-admin-edge",
    "version": "1.0.23",
    "description": "A demo package for Total TypeScript",
    "keywords": [
        "demo",
        "typescript"
    ],
    "homepage": "https://github.com/jdgamble555/firebase-admin-edge",
    "bugs": {
        "url": "https://github.com/jdgamble555/firebase-admin-edge/issues"
    },
    "author": "Jonathan Gamble",
    "repository": {
        "type": "git",
