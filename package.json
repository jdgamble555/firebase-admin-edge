    },
    "files": [
        "dist"
    ],
    "type": "module",
    "main": "dist/index.js",
    "license": "MIT",
    "devDependencies": {
        "@changesets/cli": "^2.29.7",
        "@types/node": "^24.10.1",
        "jose": "^6.1.2",
        "prettier": "^3.6.2",
        "rimraf": "^6.1.0",
        "typescript": "^5.9.3",
        "vitest": "^4.0.9"
    },
    "scripts": {
